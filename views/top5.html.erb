<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>Pageantus</title>
        <link rel="stylesheet" type="text/css" href="images/favicon.ico" />
        <link rel="stylesheet" type="text/css" href="css/projector.css" />
        <link rel="stylesheet" type="text/css" href="css/dialogbox.css" />
        <script type="text/javascript" src="js/libs/jquery.js"></script>
        <script type="text/javascript" src="js/libs/jqueryui.js"></script>
        <script type="text/javascript" src="js/libs/lodash.min.js"></script>
        <script type="text/javascript" src="js/projector.js"></script>
    </head>
    <body>
        <input type ="hidden" id="server_address" value="<%= 'http://' + request.env['REMOTE_ADDR'] + ':4567/' %>">
        <div id="container">
            <div id="idle">
                <div class="widget" id="time">
                    <div></div>
                    <div></div>
                </div>
                <div class="widget" id="unlock">
                    <div id="well">
                        <h2><strong id="slider"></strong><span>slide to unlock</span></h2>
                    </div>
                </div>
            </div>
            <div id="content">
                <div id="header">
                    <span>Mr. and Ms. University of the East 2012</span>
                    <span id="header-time"></span>
                    <span>
                        <span id="connectivity" class="tray-icon"></span>
                        <span id="lock" class="tray-icon"></span>
                    </span>
                </div>
                <div id="content-pane">
                    <table id="top-candidates" cellspacing="0" cellpadding="0">
                        <tr>
                            <td colspan="2" id="category-name"></td>
                        </tr>
                        <tr>
                            <td id="male"></td>
                            <td id="female"></td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </body>
</html>